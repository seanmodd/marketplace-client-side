{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        // "no-unused-vars": "false",
        "react/prop-types": "false",
        "react/jsx-uses-react": "false",
        "react-hooks/exhaustive-deps": "false",
        "no-use-before-define": "false"

    }
}
